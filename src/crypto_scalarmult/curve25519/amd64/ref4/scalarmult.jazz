require "curve25519.jinc"

export fn jade_scalarmult_curve25519_amd64_ref4(reg u64 rp kp up) -> reg u64
{
  reg u64 r;
  __curve25519_ref4(rp, kp, up);
  ?{}, r = #set0();
  return r;
}

export fn jade_scalarmult_curve25519_amd64_ref4_base(reg u64 rp kp) -> reg u64
{
  reg u64 r;
  __curve25519_ref4_base(rp, kp);
  ?{}, r = #set0();
  return r;
}

