require "reduce4.jinc"

inline fn __sqr4_rs(stack u64[4] x) -> reg u64[4]
{
  reg u64 zero rax rdx;
  reg u64[8] z;
  reg u64[4] r;
  reg u64[5] t;
  reg bool cf;

  z[7] = #MOV(0);
  zero = #MOV(0);

  // 2*x01 + 2*x02 + 2*x03 + 2*x12 + 2*x13 + 2*x23
  // + x00 + x11 + x22 + x33

  rax = x[1];
  rdx, rax = rax * x[0];
  z[1] = rax;
  z[2] = rdx;

  rax = x[2];
  rdx, rax = rax * x[1];
  z[3] = rax;
  z[4] = rdx;

  rax = x[3];
  rdx, rax = rax * x[2];
  z[5] = rax;
  z[6] = rdx;

  // [2*]x01 + 2*x02 + 2*x03 + [2*]x12 + 2*x13 + [2*]x23
  // + x00 + x11 + x22 + x33

  rax = x[2];
  rdx, rax = rax * x[0];
  cf, z[2] += rax;
  cf, z[3] += rdx + cf;
   _, z[4] += zero   + cf;

  rax = x[3];
  rdx, rax = rax * x[1];
  cf, z[4] += rax;
  cf, z[5] += rdx + cf;
   _, z[6] += zero   + cf;

  // [2*]x01 + [2*]x02 + 2*x03 + [2*]x12 + [2*]x13 + [2*]x23
  // + x00 + x11 + x22 + x33

  rax = x[3];
  rdx, rax = rax * x[0];
  cf, z[3] += rax;
  cf, z[4] += rdx + cf;
  cf, z[5] += zero   + cf;
  cf, z[6] += zero   + cf;
  _,  z[7] += zero   + cf;

  // x01 + x02 + x03 + x12 + x13 + x23
  // + x00 + x11 + x22 + x33

  // set z<1..2n+1> = 2*z<1..2n+1> since
  // we have summed all x_i*x_j with i<>j
  // so far and these occur twice
  cf, z[1] += z[1];
  cf, z[2] += z[2] + cf;
  cf, z[3] += z[3] + cf;
  cf, z[4] += z[4] + cf;
  cf, z[5] += z[5] + cf;
  cf, z[6] += z[6] + cf;
  cf, z[7] += z[7] + cf;

  // x00 + x11 + x22 + x33

  rax = x[0];
  rdx, rax = rax * x[0];
  z[0] = rax;
  t[0] = rdx;

  rax = x[1];
  rdx, rax = rax * x[1];
  t[1] = rax;
  t[2] = rdx;

  rax = x[2];
  rdx, rax = rax * x[2];
  t[3] = rax;
  t[4] = rdx;

  cf, z[1] += t[0];
  cf, z[2] += t[1] + cf;
  cf, z[3] += t[2] + cf;
  cf, z[4] += t[3] + cf;
  cf, z[5] += t[4] + cf;
  cf, z[6] += 0 + cf;
   _, z[7] += 0 + cf;

  rax = x[3];
  rdx, rax = rax * x[3];
  cf, z[6] += rax;
   _, z[7] += rdx + cf;

  r = __reduce4(z);

  return r;
}

inline fn __sqr4_ss(stack u64[4] fs) -> stack u64[4]
{
  stack u64[4] hs;
  reg u64[4] h;

  h = __sqr4_rs(fs);
  hs = #copy(h);

  return hs;
}

